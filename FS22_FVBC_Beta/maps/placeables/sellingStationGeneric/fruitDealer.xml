<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="sellingStation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../shared/xml/schema/placeable.xsd">
    <storeData>
        <name>Fruit Dealer</name>
        <functions>
            <function>$l10n_function_sellingPoint</function>
        </functions>
        <image>$data/placeables/lizard/sellingPoints/supermarket/store_supermarket.png</image>
        <price>80000</price>
        <lifetime>1000</lifetime>
        <rotation>0</rotation>
        <brand>NONE</brand>
        <species>placeable</species>
        <category>sellingPoints</category>
        <brush>
            <type>placeable</type>
            <category>production</category>
            <tab>sellingPoints</tab>
        </brush>
        <vertexBufferMemoryUsage>474112</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>64768</indexBufferMemoryUsage>
        <textureMemoryUsage>10289152</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
    </storeData>

    <base>
        <filename>maps/placeables/sellingStationGeneric/sellingStationNoCover2.i3d</filename>
        <canBeRenamed>true</canBeRenamed>
    </base>

    <placement useRandomYRotation="false" useManualYRotation="true" >
        <testAreas>
            <testArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
            <testArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
        </testAreas>

        <sounds>
            <place template="smallImp" />
        </sounds>
    </placement>

    <clearAreas></clearAreas>

    <leveling></leveling>

    <indoorAreas></indoorAreas>

    <tipOcclusionUpdateAreas>
        <tipOcclusionUpdateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
    </tipOcclusionUpdateAreas>

    <ai>
        <updateAreas>
            <updateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
        </updateAreas>
    </ai>

    <infoTrigger triggerNode="infoTrigger"/>

    <triggerMarkers>
        <triggerMarker node="unloadTriggerMarker" filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD" adjustToGround="true" />
    </triggerMarkers>

    <hotspots>
        <hotspot type="UNLOADING" linkNode="unloadTriggerMarker" teleportNode="unloadTriggerMarker" />
    </hotspots>



    <sellingStation appearsOnStats="true" supportsExtension="false" node="sellingStation">
        <unloadTrigger exactFillRootNode="unloadTrigger" aiNode="unloadTriggerAINode" fillTypes="POTATO FLOUR SUNFLOWER_OIL CANOLA_OIL OLIVE_OIL SUGAR STRAWBERRY LETTUCE TOMATO GRAPE GRAPEJUICE EGG RAISINS CEREAL HONEY OLIVE CHMELSUS PALLETS"/>
        <fillType name="POTATO"         priceScale="0.95" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="FLOUR"          priceScale="0.95" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="SUNFLOWER_OIL"  priceScale="0.95" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="CANOLA_OIL"     priceScale="0.95" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="OLIVE_OIL"      priceScale="0.95" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="SUGAR"          priceScale="0.95" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="STRAWBERRY"     priceScale="0.95" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="LETTUCE"        priceScale="0.95" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="TOMATO"         priceScale="0.95" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="GRAPE"          priceScale="0.95" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="GRAPEJUICE"     priceScale="0.95" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="EGG"            priceScale="0.95" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="RAISINS"        priceScale="0.95" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="CEREAL"         priceScale="0.95" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="HONEY"          priceScale="0.95" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="OLIVE"          priceScale="0.95" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="CHMELSUS"          priceScale="0.95" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="PALLETS"          priceScale="0.95" supportsGreatDemand="true" disablePriceDrop="false" />


        <sounds>
            <idle template="storeTemplate"/>
        </sounds>
    </sellingStation>

    <i3dMappings>
       
        <i3dMapping id="tipOcclusionUpdateArea1Start" node="0>11" />
        <i3dMapping id="tipOcclusionUpdateArea1End" node="0>11|0" />
        <i3dMapping id="infoTrigger" node="0>5" />
        <i3dMapping id="sellingStation" node="0>3" />
        <i3dMapping id="unloadTrigger" node="0>3" />
        <i3dMapping id="unloadTriggerMarker" node="0>7" />
        <i3dMapping id="unloadTriggerAINode" node="0>8" />
    </i3dMappings>
</placeable>
