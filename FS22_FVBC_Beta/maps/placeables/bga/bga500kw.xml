        <i3dMapping id="tipOcclusionUpdateArea1Start" node="0>5|0" />
        <i3dMapping id="tipOcclusionUpdateArea1End" node="0>5|0|0" />
        <i3dMapping id="infoTrigger" node="0>6" />
        <i3dMapping id="hotspotMarker" node="0>7" />
