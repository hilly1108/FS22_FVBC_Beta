<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="productionPoint" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shared/xml/schema/placeable.xsd">
    <storeData>
        <name>$l10n_storeItem_planET_bga500kw</name>
        <functions>
            <function>$l10n_function_productionPoint</function>
        </functions>
        <image>$data/placeables/planET/bga500kw/store_bga500kw.png</image>
        <price>80000</price>
        <lifetime>1000</lifetime>
        <rotation>0</rotation>
                   <canBeSold>true</canBeSold>
        <showInStore>false</showInStore>
        <brand>PLANET</brand>
        <species>placeable</species>
        <category>productionPoints</category>
        <brush>
            <type>placeable</type>
            <category>production</category>
            <tab>factories</tab>
        </brush>
        <vertexBufferMemoryUsage>4130816</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>619264</indexBufferMemoryUsage>
        <textureMemoryUsage>4390912</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>

        <storeIconRendering>
            <settings cameraXRot="-30" cameraYRot="150" advancedBoundingBox="true"/>
        </storeIconRendering>
        <audioMemoryUsage>0</audioMemoryUsage>
    </storeData>

    <base>
        <filename>maps/placeables/bga/bga500kw.i3d</filename>
        <canBeRenamed>true</canBeRenamed>
        <boughtWithFarmland>true</boughtWithFarmland>
    </base>

    <placement useRandomYRotation="false" useManualYRotation="true" >
        <testAreas>
            <testArea startNode="testArea1Start" endNode="testArea1End" />
            <testArea startNode="testArea2Start" endNode="testArea2End" />
            <testArea startNode="testArea3Start" endNode="testArea3End" />
        </testAreas>

        <sounds>
            <place template="largeImp" />
        </sounds>
    </placement>

    <clearAreas>
        <clearArea startNode="clearArea1Start" widthNode="clearArea1Width" heightNode="clearArea1Height"/>
        <clearArea startNode="clearArea2Start" widthNode="clearArea2Width" heightNode="clearArea2Height"/>
        <clearArea startNode="clearArea3Start" widthNode="clearArea3Width" heightNode="clearArea3Height"/>
    </clearAreas>

    <leveling requireLeveling="true" maxSmoothDistance="10" maxSlope="75" maxEdgeAngle="30" >
        <levelAreas>
            <levelArea startNode="clearArea1Start" widthNode="clearArea1Width" heightNode="clearArea1Height" groundType="asphalt"/>
            <levelArea startNode="clearArea2Start" widthNode="clearArea2Width" heightNode="clearArea2Height" groundType="asphalt"/>
            <levelArea startNode="clearArea3Start" widthNode="clearArea3Width" heightNode="clearArea3Height" groundType="asphalt"/>
        </levelAreas>
    </leveling>

    <indoorAreas>
        <indoorArea startNode="indoorArea1Start" widthNode="indoorArea1Width" heightNode="indoorArea1Height"/>
        <indoorArea startNode="indoorArea2Start" widthNode="indoorArea2Width" heightNode="indoorArea2Height"/>
        <indoorArea startNode="indoorArea3Start" widthNode="indoorArea3Width" heightNode="indoorArea3Height"/>
        <indoorArea startNode="indoorArea4Start" widthNode="indoorArea4Width" heightNode="indoorArea4Height"/>
        <indoorArea startNode="indoorArea5Start" widthNode="indoorArea5Width" heightNode="indoorArea5Height"/>
    </indoorAreas>

    <tipOcclusionUpdateAreas>
        <tipOcclusionUpdateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
    </tipOcclusionUpdateAreas>

    <ai>
        <updateAreas>
            <updateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
        </updateAreas>
    </ai>

    <infoTrigger triggerNode="infoTrigger"/>

    <triggerMarkers>
        <triggerMarker node="unloadTriggerMarker"             filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD" adjustToGround="false" />
        <triggerMarker node="unloadTriggerLiquidManureMarker" filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD" adjustToGround="true" />
        <triggerMarker node="loadTriggerMarker1"              filename="$data/shared/assets/marker/markerIcons.xml" id="LOAD"   adjustToGround="true" />
    
        <triggerMarker node="playerTriggerMarker"             filename="$data/shared/assets/marker/markerIcons.xml" id="WRENCH" adjustToGround="true" />
    </triggerMarkers>

    <hotspots>
        <hotspot type="PRODUCTION_POINT" linkNode="hotspotMarker" teleportNode="teleportNode"/>
    </hotspots>

    <productionPoint>
        <productions sharedThroughputCapacity="false">
            <production id="biogas" name="$l10n_fillType_silage"  cyclesPerHour="1" costsPerActiveHour="12.6">
                <inputs>
                    <input fillType="SILAGE" amount="4243" />
                </inputs>
                <outputs>
                    <output fillType="ELECTRICCHARGE" amount="3819"   sellDirectly="true"/>
                    <output fillType="METHANE"        amount="5"    sellDirectly="true"/>
                    <output fillType="DIGESTATE"      amount="1273" />
                </outputs>
            </production>
            <production id="biogasLiquidManure" name="$l10n_fillType_liquidManure" cyclesPerHour="1" costsPerActiveHour="12.6">
                <inputs>
                    <input fillType="LIQUIDMANURE" amount="1010" />
                </inputs>
                <outputs>
                    <output fillType="ELECTRICCHARGE" amount="909"   sellDirectly="true"/>
                    <output fillType="METHANE"        amount="5"     sellDirectly="true"/>
                    <output fillType="DIGESTATE"      amount="303" />
                </outputs>
            </production>
            <production id="biogasManure" name="$l10n_fillType_manure" cyclesPerHour="1" costsPerActiveHour="12.6">
                <inputs>
                    <input fillType="MANURE" amount="1010" />
                </inputs>
                <outputs>
                    <output fillType="ELECTRICCHARGE" amount="909"   sellDirectly="true"/>
                    <output fillType="METHANE"        amount="5"     sellDirectly="true"/>
                    <output fillType="DIGESTATE"      amount="303" />
                </outputs>
            </production>
            <production id="biogasSugarbeetCut" name="$l10n_fillType_sugarBeetCut" cyclesPerHour="1" costsPerActiveHour="12.6">
                <inputs>
                    <input fillType="SUGARBEET_CUT" amount="505" />
                </inputs>
                <outputs>
                    <output fillType="ELECTRICCHARGE" amount="456"  sellDirectly="true"/>
                    <output fillType="METHANE"        amount="5"    sellDirectly="true"/>
                    <output fillType="DIGESTATE"      amount="152" />
                </outputs>
            </production>
        </productions>

        <sellingStation node="sellingStation" appearsOnStats="true" supportsExtension="true">
            <unloadTrigger exactFillRootNode="unloadTriggerLiquidManure" fillTypes="LIQUIDMANURE" aiNode="unloadTriggerLiquidManureAiNode"/>

            <unloadTrigger exactFillRootNode="unloadTrigger"             fillTypes="SILAGE MANURE SUGARBEET_CUT" />
            <unloadTrigger fillTypes="SILAGE SUGARBEET_CUT">
                <baleTrigger triggerNode="baleTrigger" />
            </unloadTrigger>

            <unloadTrigger exactFillRootNode="unloadTriggerInFront"      fillTypes="SILAGE MANURE SUGARBEET_CUT" aiNode="unloadTriggerInFrontAINode" />
            <unloadTrigger fillTypes="SILAGE SUGARBEET_CUT">
                <baleTrigger triggerNode="baleTriggerInFront" />
            </unloadTrigger>
        </sellingStation>

        <loadingStation fillTypes="MANURE LIQUIDMANURE" supportsExtension="true">
            <loadTrigger triggerNode="loadTrigger1" fillTypes="DIGESTATE" />
           
        </loadingStation>

        <storage isExtension="false" fillLevelSyncThreshold="100">
            <capacity fillType="SILAGE"        capacity="152750" />
            <capacity fillType="MANURE"        capacity="36370" />
            <capacity fillType="SUGARBEET_CUT" capacity="18180" />
            <capacity fillType="LIQUIDMANURE"  capacity="152750" />
            <capacity fillType="DIGESTATE"     capacity="5600000" />

            <dynamicFillPlane node="bunkerFillVolume" allSidePlanes="false" maxAllowedHeapAngle="10" maxDelta="0.3" defaultFillType="SILAGE" />
        </storage>

        <playerTrigger node="playerTrigger" />

        <sounds>
            <idle   template="dairyIdle" linkNode="0>" outerRadius="50"/>
            <active template="dairyWork" linkNode="0>" outerRadius="50"/>
        </sounds>
    </productionPoint>

    <i3dMappings>
        <i3dMapping id="clearArea1Start" node="0>0|0" />
        <i3dMapping id="clearArea1Width" node="0>0|0|0" />
        <i3dMapping id="clearArea1Height" node="0>0|0|1" />
