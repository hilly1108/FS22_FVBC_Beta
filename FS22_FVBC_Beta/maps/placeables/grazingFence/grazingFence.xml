<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="simplePlaceable" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../shared/xml/schema/placeable.xsd">
    <storeData>
        <name>$l10n_construction_category_decoration</name>
        <image>$data/placeables/mapUS/farmBuildings/farmShed/store_farmShed.png</image>
        <canBeSold>true</canBeSold>
        <showInStore>false</showInStore>
        <price>10</price>
        <lifetime>1000</lifetime>
        <rotation>0</rotation>
        <brand>NONE</brand>
        <species>placeable</species>
        <category>farmhouses</category>
        <brush>
            <type>placeable</type>
            <category>buildings</category>
            <tab>farmhouses</tab>
        </brush>
        <vertexBufferMemoryUsage>0</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>0</indexBufferMemoryUsage>
        <textureMemoryUsage>0</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>

    <base>
        <filename>maps/placeables/grazingFence/grazingFence.i3d</filename>
          <canBeRenamed>true</canBeRenamed>
        <boughtWithFarmland>true</boughtWithFarmland>
		
    </base>

    <placement useRandomYRotation="false" useManualYRotation="true" >
    </placement>

    <tipOcclusionUpdateAreas>
    </tipOcclusionUpdateAreas>

    <clearAreas>
    </clearAreas>

    <leveling>
        <levelAreas >
        </levelAreas>
    </leveling>

    <indoorAreas>
    </indoorAreas>

    <ai>
        <updateAreas>
           
        </updateAreas>
    </ai>
	
<animatedObjects>
     <animatedObject saveId="grazingFence" >
            <animation duration="2">
                <part node="0|0">
                    <keyFrame time="0.0" translation="0 -1.8 0" />
                    <keyFrame time="1" translation="0 0 0" />
                </part>			
			</animation>
            <controls triggerNode="0|1" posAction="ACTIVATE_HANDTOOL" posText="action_openGate" negText="action_closeGate" />
            <sounds>
                <moving linkNode="0|1" template="cowLayer"/>
                <posEnd linkNode="0|1" template="slidingGateMetalOpen" />
                <negEnd linkNode="0|1" template="slidingGateMetalClose" />
            </sounds>
        </animatedObject>
		
</animatedObjects>



</placeable>

