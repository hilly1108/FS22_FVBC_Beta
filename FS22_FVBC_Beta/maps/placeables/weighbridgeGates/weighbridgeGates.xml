<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="simplePlaceable" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../shared/xml/schema/placeable.xsd">
    <storeData>
        <name>$l10n_construction_category_decoration</name>
        <image>$data/placeables/mapUS/farmBuildings/farmShed/store_farmShed.png</image>
        <canBeSold>true</canBeSold>
        <showInStore>false</showInStore>
        <price>10</price>
        <lifetime>1000</lifetime>
        <rotation>0</rotation>
        <brand>NONE</brand>
        <species>placeable</species>
        <category>farmhouses</category>
        <brush>
            <type>placeable</type>
            <category>buildings</category>
            <tab>farmhouses</tab>
        </brush>
        <vertexBufferMemoryUsage>0</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>0</indexBufferMemoryUsage>
        <textureMemoryUsage>0</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>

    <base>
        <filename>maps/placeables/weighbridgeGates/weighbridgeGates.i3d</filename>
           <boughtWithFarmland>true</boughtWithFarmland>
		
    </base>

    <placement useRandomYRotation="false" useManualYRotation="true" >
    </placement>


    <tipOcclusionUpdateAreas>
    </tipOcclusionUpdateAreas>

    <clearAreas>
    </clearAreas>

    <leveling>
        <levelAreas >
        </levelAreas>
    </leveling>

    <indoorAreas>
    </indoorAreas>

    <ai>
        <updateAreas>
           
        </updateAreas>
    </ai>
<animatedObjects>
	<animatedObject>
		<animation duration="6">
			<part node="0>0|1"> <!--Front Gate-->
				<keyFrame time="0.24" rotation="0 0 0" />
				<keyFrame time="1" rotation="0 0 85" />
			</part>
			<part node="0>0|2"> <!--Back Gate-->
                <keyFrame time="0.24" rotation="0 0 95" />
				<keyFrame time="1" rotation="0 0 180" />
			</part>
		</animation>
		<controls triggerNode="0>0|0" posAction="ACTIVATE_HANDTOOL" posText="action_openGate" negText="action_closeGate" />
		<sounds>
			<moving linkNode="0>0|2" template="fenceMetalLoop" />
			<posEnd linkNode="0>0|2" template="fenceMetalOpen" />
			<negEnd linkNode="0>0|2" template="fenceMetalClose" />
            <moving linkNode="0>0|1" template="fenceMetalLoop" />
			<posEnd linkNode="0>0|1" template="fenceMetalOpen" />
			<negEnd linkNode="0>0|1" template="fenceMetalClose" />
		</sounds>
	</animatedObject>
	</animatedObjects>
</placeable>
