<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="productionPoint" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../shared/xml/schema/placeable.xsd">
    <storeData>
        <name>Concrete Production</name>
        <functions>
            <function>$l10n_function_sellingPoint</function>
        </functions>
        <image>>$data/store/store_empty.png</image>
        <price>1000000</price>
        <lifetime>1000</lifetime>
        <rotation>0</rotation>
        <brand>LIZARD</brand>
        <species>placeable</species>
        <category>productionPoints</category>
        <brush>
            <type>placeable</type>
            <category>production</category>
            <tab>factories</tab>
        </brush>
		 <canBeSold>true</canBeSold>
        <showInStore>false</showInStore>
        <vertexBufferMemoryUsage>1080832</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>181504</indexBufferMemoryUsage>
        <textureMemoryUsage>4194304</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
        <storeIconRendering>
            <settings cameraXRot="-25" cameraYRot="30" advancedBoundingBox="true"/>
        </storeIconRendering>
        <audioMemoryUsage>0</audioMemoryUsage>
    </storeData>

    <base>
        <filename>maps/placeables/debrisCrusherCn/debrisCrusherCn.i3d</filename>
        <canBeRenamed>true</canBeRenamed>
                <boughtWithFarmland>false</boughtWithFarmland>
    </base>

    <placement useRandomYRotation="false" useManualYRotation="true" >
        <testAreas>
            <testArea startNode="testArea1Start" endNode="testArea1End" />
        </testAreas>
    </placement>

    <clearAreas>
        <clearArea startNode="clearArea1Start" widthNode="clearArea1Width" heightNode="clearArea1Height"/>
        <clearArea startNode="clearArea2Start" widthNode="clearArea2Width" heightNode="clearArea2Height"/>
    </clearAreas>

    <leveling requireLeveling="true" maxSmoothDistance="10" maxSlope="45" maxEdgeAngle="10" >
        <levelAreas>
            <levelArea startNode="levelArea1Start" widthNode="levelArea1Width" heightNode="levelArea1Height" groundType="asphalt"/>
            <levelArea startNode="levelArea2Start" widthNode="levelArea2Width" heightNode="levelArea2Height" groundType="asphalt"/>
        </levelAreas>
    </leveling>

    <indoorAreas/>

    <tipOcclusionUpdateAreas>
        <tipOcclusionUpdateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
    </tipOcclusionUpdateAreas>

    <ai>
        <updateAreas>
            <updateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
        </updateAreas>
    </ai>

    <infoTrigger triggerNode="infoTrigger"/>

    <triggerMarkers>
        <triggerMarker node="0>3|0|4" filename="$data/shared/assets/marker/markerIcons.xml" id="LOAD" />
		<triggerMarker node="playerTriggerMarker" 	   filename="$data/shared/assets/marker/markerIcons.xml" id="WRENCH" adjustToGround="true" />
        <triggerMarker node="unloadTriggerMarker" 	   filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD" adjustToGround="true" />
    </triggerMarkers>

    <hotspots>
        <hotspot type="PRODUCTION_POINT" linkNode="playerTriggerMarker" teleportNode="teleportNode" />
    </hotspots>
	
	<productionPoint>
		<productions sharedThroughputCapacity="false"> 
			<production id="Concrete Production" name="Concrete Production" cyclesPerHour="1" costsPerActiveHour="5">
                <inputs>
                    <input fillType="CEMENT" amount="100" />
                    <input fillType="GRAVEL" amount="300" />
                    <input fillType="WATER" amount="50" />
					
                </inputs>
                <outputs>
                    <output fillType="CONCRETE" amount="650" />
                   
                </outputs>
            </production>
   		</productions>


    <animationNodes>
        <animationNode class="RotationAnimation" node="roll" rotSpeed="-20" rotAxis="1" />
    </animationNodes>
    
    <effectNodes>
     

    </effectNodes>     


    <sellingStation appearsOnStats="true" node="sellingStation" supportsExtension="true" >
        <unloadTrigger exactFillRootNode="unloadTriggerUpper" fillTypes="CEMENT GRAVEL"  /> 
        <unloadTrigger exactFillRootNode="unloadTriggerWater" fillTypes="WATER"/> 
        <fillType name="CEMENT"         priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="GRAVEL"          priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
        <fillType name="WATER"          priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
        <sounds>
            <active template="debrisCrusherWork" linkNode="roll" />
			<active template="DEFAULT_REFUEL_SOUND" linkNode="unloadTriggerWater" />
        </sounds>
        
        <effectNodes>
            <effectNode effectNode="effectSmoke" materialType="unloadingSmoke" fadeTime="1.5" />
            <effectNode effectClass="PipeEffect" effectNode="0>0|10|1|1"      materialType="pipe"           fadeTime="0.5" maxBending="0" controlPoint="4.8 0 0 0" defaultFillType="GRAVEL" dynamicFillType="true"/>
            <effectNode effectClass="PipeEffect" effectNode="0>0|10|1|2"      materialType="pipe"           fadeTime="0.5" maxBending="0" controlPoint="4.8 0 0 0" defaultFillType="GRAVEL" dynamicFillType="true"/>
            <effectNode effectClass="PipeEffect" effectNode="0>0|10|1|3"      materialType="pipe"           fadeTime="0.5" maxBending="0" controlPoint="4.8 0 0 0" defaultFillType="GRAVEL" dynamicFillType="true"/>
            <effectNode effectClass="PipeEffect" effectNode="0>0|10|1|4"      materialType="pipe"           fadeTime="0.5" maxBending="0" controlPoint="4.8 0 0 0" defaultFillType="GRAVEL" dynamicFillType="true"/>
        </effectNodes>
    </sellingStation>
     
 	       <loadingStation node="loadingTrigger">
			<loadTrigger triggerNode="loadingTrigger" fillLitersPerSecond="1000" dischargeNode="dischargeNode" fillTypes="CONCRETE" aiNode="aiLoadingNode">     
            <effectNode effectClass="PipeEffect" effectNode="pipeEffect"      materialType="pipe"           fadeTime="0.5" maxBending="0" controlPoint="4.8 0 0 0"/>
            <effectNode effectClass="PipeEffect" effectNode="pipeEffect1"      materialType="pipe"           fadeTime="0.5" maxBending="0" controlPoint="4.8 0 0 0"/>
            <effectNode                          effectNode="pipeEffectSmoke" materialType="unloadingSmoke" fadeTime="0.5" />        
            </loadTrigger>
            
        </loadingStation>

        <storage isExtension="false" fillLevelSyncThreshold="100">            
			<capacity fillType="CEMENT"           capacity="5000" />
            <capacity fillType="GRAVEL"            capacity="5000" />
            <capacity fillType="WATER"            capacity="50000" />
            <capacity fillType="CONCRETE"            capacity="20000" />
           
        </storage>
		<playerTrigger node="playerTrigger" />
	
	</productionPoint>	
	
	<incomePerHour>0</incomePerHour>

   <i3dMappings>

		<!--unloading-->
		<i3dMapping id="dischargeNode" node="0>3|0|1" />
        <i3dMapping id="unloadTriggerWater" node="0>1|0" />
        <i3dMapping id="unloadTriggerMarker" node="0>1|1" />
		<i3dMapping id="clearArea1Start" node="0>0|0|0" />
        <i3dMapping id="clearArea1Width" node="0>0|0|0|0" />
        <i3dMapping id="clearArea1Height" node="0>0|0|0|1" />
        <i3dMapping id="clearArea2Start" node="0>0|0|1" />
        <i3dMapping id="clearArea2Width" node="0>0|0|1|0" />
        <i3dMapping id="clearArea2Height" node="0>0|0|1|1" />
        <i3dMapping id="levelArea1Start" node="0>0|1|0" />
        <i3dMapping id="levelArea1Width" node="0>0|1|0|0" />
        <i3dMapping id="levelArea1Height" node="0>0|1|0|1" />
        <i3dMapping id="levelArea2Start" node="0>0|1|1" />
        <i3dMapping id="levelArea2Width" node="0>0|1|1|0" />
        <i3dMapping id="levelArea2Height" node="0>0|1|1|1" />
        <i3dMapping id="testArea1Start" node="0>0|3|0" />
        <i3dMapping id="testArea1End" node="0>0|3|0|0" />
        <i3dMapping id="tipOcclusionUpdateArea1Start" node="0>0|4|0" />
        <i3dMapping id="tipOcclusionUpdateArea1End" node="0>0|4|0|0" />
        <i3dMapping id="unloadTriggerUpper" node="0>0|7|0" />
 <!--       <i3dMapping id="unloadTriggerUpperMarker" node="0>0|7|1" />-->
        <i3dMapping id="teleportNode" node="0>0|8" />
        <i3dMapping id="roll" node="0>0|9|5" />
        <i3dMapping id="aiLoadingNode" node="0>1|2" />
        <i3dMapping id="loadMarker" node="0>3|0|4" />
        <i3dMapping id="pipeEffect" node="0>3|0|2|0" />
        <i3dMapping id="pipeEffect1" node="0>3|0|2|2" />
        <i3dMapping id="pipeEffectSmoke" node="0>3|0|2|1" />
        <i3dMapping id="effectSmoke" node="0>0|10|0" />
		<i3dMapping id="loadingTrigger" node="0>3|0|0" />
        <i3dMapping id="aiNodeLoading" node="0>3|0|3" />
        <i3dMapping id="infoTrigger" node="0>0|5" />
		<i3dMapping id="playerTrigger" node="0>2|0" />
		<i3dMapping id="playerTriggerMarker" node="0>2|0|0" />
        </i3dMappings>
</placeable>
