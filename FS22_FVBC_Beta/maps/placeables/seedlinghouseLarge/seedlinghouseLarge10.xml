<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="greenhouse" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../shared/xml/schema/placeable.xsd">
    <storeData>
        <name>Seedling House 10</name>
        <functions>
            <function>$l10n_function_productionPoint</function>
        </functions>
        <image>$data/placeables/lizard/greenhouses/greenhouseLarge/store_greenhouseLarge.png</image>
        <price>1000</price>
		        <canBeSold>true</canBeSold>
        <showInStore>false</showInStore>
        <dailyUpkeep>100</dailyUpkeep>
        <lifetime>1000</lifetime>
        <rotation>0</rotation>
        <brand>NONE</brand>
        <species>placeable</species>
        <category>placeableMisc</category>
        <brush>
            <type>placeable</type>
            <category>production</category>
            <tab>greenhouses</tab>
        </brush>
        <canBeSold>true</canBeSold>
        <vertexBufferMemoryUsage>991744</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>149504</indexBufferMemoryUsage>
        <textureMemoryUsage>5177344</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
        <storeIconRendering>
            <settings cameraXRot="-20" cameraYRot="30" advancedBoundingBox="true"/>
        </storeIconRendering>
        <audioMemoryUsage>0</audioMemoryUsage>
    </storeData>

    <base>
        <filename>maps/placeables/seedlinghouseLarge/seedlinghouseLarge.i3d</filename>
        <canBeRenamed>true</canBeRenamed>
		<boughtWithFarmland>true</boughtWithFarmland>
    </base>

    <placement useRandomYRotation="false" useManualYRotation="true" >
        <testAreas>
            <testArea startNode="testArea1Start" endNode="testArea1End" />
        </testAreas>

        <sounds>
            <place template="smallImp" />
        </sounds>
    </placement>

    <clearAreas>
        <clearArea startNode="clearArea1Start" widthNode="clearArea1Width" heightNode="clearArea1Height"/>
    </clearAreas>

    <leveling requireLeveling="true" maxSmoothDistance="1" maxSlope="75" maxEdgeAngle="30">
        <levelAreas>
            <levelArea startNode="levelArea1Start" widthNode="levelArea1Width" heightNode="levelArea1Height"/>
        </levelAreas>
    </leveling>

    <indoorAreas>
        <indoorArea startNode="indoorArea1Start" widthNode="indoorArea1Width" heightNode="indoorArea1Height"/>
    </indoorAreas>

    <tipOcclusionUpdateAreas>
        <tipOcclusionUpdateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
    </tipOcclusionUpdateAreas>

    <ai>
        <updateAreas>
            <updateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
        </updateAreas>
    </ai>

    <infoTrigger triggerNode="infoTrigger" />

    <hotspots>
        <hotspot type="PRODUCTION_POINT" linkNode="unloadTriggerMarker" teleportNode="teleportNode" />
    </hotspots>

    <triggerMarkers>
        <triggerMarker node="warningStripes" />
        <triggerMarker node="playerTriggerMarker" filename="$data/shared/assets/marker/markerIcons.xml" id="WRENCH" adjustToGround="true" />
        <triggerMarker node="unloadTriggerMarker" filename="$data/shared/assets/marker/markerIcons.xml" id="WATER"  adjustToGround="false" />
		<triggerMarker node="loadTriggerMarker" filename="$data/shared/assets/marker/markerIcons.xml" id="EXCLAMATION_MARK" adjustToGround="true" />
    </triggerMarkers>

    <greenhouse>
        <plants>
            <plant fillType="SEEDLINGS"    xmlFilename="maps/placeables/seedlinghouseLarge/seedlings/seedlings.xml" />
             <plant fillType="DESIGNER_PLANTS"    xmlFilename="maps/placeables/marketGarden/designer_plants1/designer_plants1.xml" />
         
        </plants>

        <plantSpaces>
            <spacesParent node="plantNodes" useRandomYRot="false" />
        </plantSpaces>

        <sounds>
            <watering template="DEFAULT_SPRAYER" linkNode="0>" loops="0"/>
        </sounds>

    </greenhouse>

    <productionPoint>
        <productions>
            <production id="seedling" name="Seedlings" cyclesPerHour="40" costsPerActiveHour="0.1">
                <inputs>
					<input fillType="SEEDS" amount="1" />
					<input fillType="MANURE" amount="3" />
					<input fillType="STRAW" amount="3" />
					<input fillType="LIQUIDFERTILIZER" amount="0.5" />	
					<input fillType="HERBICIDE" amount="0.5" />	
 
                </inputs>
                <outputs>
                    <output fillType="SEEDLINGS" amount="20" />
                </outputs>
            </production>
   
            
           <production id="designer_plants" name="Designer Plants" cyclesPerHour="10" costsPerActiveHour="0.2">
                <inputs>
                  
					<input fillType="SEEDS" amount="2" />
					<input fillType="MANURE" amount="4" />
					<input fillType="STRAW" amount="4" />	
                </inputs>
                <outputs>
                    <output fillType="DESIGNER_PLANTS" amount="15" />
                </outputs>
            </production>
          
            
          
        </productions>

        <sellingStation node="sellingStation" appearsOnStats="true" supportsExtension="false">
            <unloadTrigger exactFillRootNode="unloadTrigger" fillTypes="LIQUIDFERTILIZER HERBICIDE SEEDS MANURE STRAW" aiNode="unloadTriggerAINode" />
        </sellingStation>
		
		   <palletSpawner node="palletSpawner">
            <spawnPlaces>
                <spawnPlace startNode="spawnPlace1Start" />
            </spawnPlaces>
        </palletSpawner>
		
		   <storage node="storage" isExtension="false" fillTypes="SEEDLINGS SEEDS MANURE LIQUIDFERTILIZER HERBICIDE STRAW" fillLevelSyncThreshold="20" >

		    <capacity fillType="SEEDLINGS"     capacity="20000" />
			<capacity fillType="SEEDS"      capacity="20000" />
			<capacity fillType="MANURE"      capacity="20000" />
			<capacity fillType="LIQUIDFERTILIZER"      capacity="20000" />
			<capacity fillType="HERBICIDE"      capacity="20000" />
			<capacity fillType="STRAW"      capacity="20000" />
          
            <capacity fillType="DESIGNER_PLANTS"      capacity="20000" />
          
        </storage>
		
		<playerTrigger node="playerTrigger" />


  
    </productionPoint>

    <animatedObjects>
