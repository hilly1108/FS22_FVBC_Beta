<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="productionPoint" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shared/xml/schema/placeable.xsd">
    <storeData>
		<name>
			<en>Industrial BGA</en>
			<de>Industrie-BGA</de>
		</name>
        <functions>
            <function>$l10n_function_productionPoint</function>
        </functions>
        <image>maps/placeables/FS22_industrial_BGA/store/store_IBGA.dds</image>
        <price>50000</price>
        <dailyUpkeep>250</dailyUpkeep>
        <lifetime>1000</lifetime>
           <canBeSold>false</canBeSold>
        <showInStore>false</showInStore>
        <rotation>0</rotation>
        <brand>LIZARD</brand>
        <species>placeable</species>
        <category>productionPoints</category>
        <brush>
            <type>placeable</type>
            <category>production</category>
            <tab>factories</tab>
        </brush>

        <audioMemoryUsage>0</audioMemoryUsage>
        <vertexBufferMemoryUsage>5484288</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>1207040</indexBufferMemoryUsage>
        <textureMemoryUsage>6553600</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>

    <base>
        <filename>maps/placeables/FS22_industrial_BGA/i3d/industrialBGA.i3d</filename>
        <canBeRenamed>true</canBeRenamed>
        <boughtWithFarmland>true</boughtWithFarmland>
    </base>

    <placement useRandomYRotation="false" useManualYRotation="true" >
        <testAreas>
            <testArea startNode="testArea1Start" endNode="testArea1End" />
      
        </testAreas>

        <sounds>
            <place template="medImp" />
        </sounds>
    </placement>

    <clearAreas>

        <clearArea startNode="clearAreaStart" widthNode="clearAreaWidth" heightNode="clearAreaHeight"/>

 </clearAreas>

    <leveling requireLeveling="true" maxSmoothDistance="10" maxSlope="75" maxEdgeAngle="30" >
        <levelAreas>

    

            <levelArea startNode="levelArea1Start" widthNode="levelArea1Width" heightNode="levelArea1Height" groundType="asphalt"/>
        </levelAreas>
    </leveling>

    <indoorAreas>
        <indoorArea startNode="indoorArea1Start" widthNode="indoorArea1Width" heightNode="indoorArea1Height"/>
     
    </indoorAreas>

    <tipOcclusionUpdateAreas>
        <tipOcclusionUpdateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
    </tipOcclusionUpdateAreas>

    <ai>
        <updateAreas>
            <updateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
        </updateAreas>
    </ai>

    <infoTrigger triggerNode="infoTrigger"/>

    <triggerMarkers>
        <triggerMarker node="unloadTriggerBunkerMarker"       filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD" adjustToGround="false" />
        <triggerMarker node="unloadTriggerLiquidManureMarker" filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD" adjustToGround="true" />
        <triggerMarker node="loadTriggerMarker"               filename="$data/shared/assets/marker/markerIcons.xml" id="LOAD"   adjustToGround="true" />
        <triggerMarker node="playerTriggerMarker"             filename="$data/shared/assets/marker/markerIcons.xml" id="WRENCH" adjustToGround="true" />
    </triggerMarkers>

    <hotspots>
        <hotspot type="PRODUCTION_POINT" linkNode="hotspotMarker" teleportNode="teleportNode"/>
    </hotspots>

    <productionPoint>
        <productions sharedThroughputCapacity="false">
            <production id="biogas" name="$l10n_fillType_silage" cyclesPerHour="100" costsPerActiveHour="6.3">
                <inputs>
                    <input fillType="SILAGE" amount="2120" />
                </inputs>
                <outputs>
                    <output fillType="ELECTRICCHARGE" amount="2033" />
                    <output fillType="METHANE"        amount="30"    />
                    <output fillType="DIGESTATE"      amount="636" />
                </outputs>
            </production>
            <production id="biogasLiquidManure" name="$l10n_fillType_liquidManure" cyclesPerHour="100" costsPerActiveHour="6.3">
                <inputs>
                    <input fillType="LIQUIDMANURE" amount="505" />
                </inputs>
                <outputs>
                    <output fillType="ELECTRICCHARGE" amount="475"  />
                    <output fillType="METHANE"        amount="1"    />
                    <output fillType="DIGESTATE"      amount="152" />
                </outputs>
            </production>
            <production id="biogasManure" name="$l10n_fillType_manure" cyclesPerHour="100" costsPerActiveHour="6.3">
                <inputs>
                    <input fillType="MANURE" amount="505" />
                </inputs>
                <outputs>
                    <output fillType="ELECTRICCHARGE" amount="475" />
                    <output fillType="METHANE"        amount="1"   />
                    <output fillType="DIGESTATE"      amount="152" />
                </outputs>
            </production>
            <production id="biogasSugarbeetCut" name="$l10n_fillType_sugarBeetCut" cyclesPerHour="100" costsPerActiveHour="6.3">
                <inputs>
                    <input fillType="SUGARBEET_CUT" amount="253" />
                </inputs>
                <outputs>
                    <output fillType="ELECTRICCHARGE" amount="240" />
                    <output fillType="METHANE"        amount="20"    />
                    <output fillType="DIGESTATE"      amount="76" />
                </outputs>
            </production>
            <production id="biogasSugarbeet" name="$l10n_fillType_sugarBeet" cyclesPerHour="100" costsPerActiveHour="2.5">
                <inputs>
                    <input fillType="SUGARBEET" amount="253" />
                </inputs>
                <outputs>
                    <output fillType="ELECTRICCHARGE" amount="240"/>
                    <output fillType="METHANE"        amount="20" />
                    <output fillType="DIGESTATE"      amount="76" />
                </outputs>
            </production>			
 
			<production id="biogasPotato" name="$l10n_fillType_potato" cyclesPerHour="100" costsPerActiveHour="2.5">
                <inputs>
                    <input fillType="POTATO" amount="253" />
                </inputs>
                <outputs>
                    <output fillType="ELECTRICCHARGE" amount="240" />
                    <output fillType="METHANE"        amount="10"  />
                    <output fillType="DIGESTATE"      amount="76" />
                </outputs>
            </production>				
            <production id="biogasStraw" name="$l10n_fillType_straw" cyclesPerHour="100" costsPerActiveHour="2.5">
                <inputs>
                    <input fillType="straw" amount="1680" />
                </inputs>
                <outputs>
                    <output fillType="ELECTRICCHARGE" amount="160" />
                    <output fillType="METHANE"        amount="105"   />
                    <output fillType="DIGESTATE"      amount="240" />
                </outputs>
            </production>			
 
			<production id="biogasSilageslurry" name="$l10n_SilageAndSlurry" cyclesPerHour="120" costsPerActiveHour="2.5">
                <inputs>
                    <input fillType="SILAGE" amount="2120" />
                    <input fillType="LIQUIDMANURE" amount="505" />
					
                </inputs>
                <outputs>
                    <output fillType="ELECTRICCHARGE" amount="2550" />
                    <output fillType="METHANE"        amount="400"    />
                    <output fillType="DIGESTATE"      amount="1000" />
                </outputs>
            </production>				
            <production id="biogasStrawslurry" name="$l10n_StrawAndSlurry" cyclesPerHour="120" costsPerActiveHour="2.5">
                <inputs>
                    <input fillType="straw" amount="1680" />
                    <input fillType="LIQUIDMANURE" amount="505" />
 
				</inputs>
                <outputs>
                    <output fillType="ELECTRICCHARGE" amount="735" />
                    <output fillType="METHANE"        amount="200"   />
                    <output fillType="DIGESTATE"      amount="1000" />
                </outputs>
            </production>		

			
        </productions>

        <sellingStation node="sellingStation" appearsOnStats="true" supportsExtension="true">
            <unloadTrigger exactFillRootNode="unloadTriggerLiquidManure" fillTypes="LIQUIDMANURE" aiNode="unloadTriggerLiquidManureAiNode" />

            <unloadTrigger exactFillRootNode="unloadTriggerBunker"       fillTypes="SILAGE MANURE SUGARBEET_CUT SUGARBEET POTATO STRAW" />
            <unloadTrigger fillTypes="SILAGE STRAW">
                <baleTrigger triggerNode="baleTriggerBunker" />
            </unloadTrigger>

            <unloadTrigger exactFillRootNode="unloadTriggerInFront"      fillTypes="SILAGE MANURE SUGARBEET_CUT SUGARBEET POTATO STRAW" aiNode="unloadTriggerInFrontAINode" />
            <unloadTrigger fillTypes="SILAGE STRAW">
                <baleTrigger triggerNode="baleTriggerInFront" />
            </unloadTrigger>
        </sellingStation>

		<chargingStation>
			<chargeIndicator node="chargeFlash" light="chargeLight"/>
			<sounds>
				<fill
					template="ELECTRIC_CHARGING_STATION" innerRadius="3" outerRadius="30" volumeScale="1" pitchScale="1" >
				</fill>
			</sounds>
		</chargingStation>

        <loadingStation>
            <loadTrigger triggerNode="loadTrigger" fillTypes="DIGESTATE" aiNode="loadTriggerAiNode"/>
            <loadTrigger triggerNode="chargeTrigger" fillTypes="electricCharge" aiNode="chargingAInode" fillLitersPerSecond="20" autoStart="true" infiniteCapacity="true" startFillText="$l10n_action_startCharging" stopFillText="$l10n_action_stopCharging"/>
			<loadTrigger triggerNode="fillTrigger" fillTypes="METHANE" fillLitersPerSecond="30" autoStart="true" infiniteCapacity="true" />
  
		</loadingStation>

        <storage isExtension="false" fillLevelSyncThreshold="100">
            <capacity fillType="SILAGE"        capacity="500000" />
            <capacity fillType="MANURE"        capacity="500000" />
            <capacity fillType="SUGARBEET_CUT" capacity="90000" />
            <capacity fillType="SUGARBEET" 	   capacity="90000" />
            <capacity fillType="POTATO" 	   capacity="90000" />			
            <capacity fillType="LIQUIDMANURE"  capacity="500000" />
            <capacity fillType="DIGESTATE"     capacity="10000000" />
            <capacity fillType="ELECTRICCHARGE"     capacity="1000000" />			
            <capacity fillType="METHANE"     capacity="1000000" />			
            <capacity fillType="STRAW"        capacity="500000" />
			

            <dynamicFillPlane node="bunkerFillVolume" allSidePlanes="false" maxAllowedHeapAngle="10" maxDelta="0.3" defaultFillType="SILAGE" />
        </storage>

        <playerTrigger node="playerTrigger" />

        <sounds>
            <idle template="dairyIdle" linkNode="0>" outerRadius="50"/>
            <active template="dairyWork" linkNode="0>" outerRadius="50"/>
        </sounds>
    </productionPoint>

    <i3dMappings>
        <i3dMapping id="clearAreaStart" node="0>0|0|0" />
        <i3dMapping id="clearAreaWidth" node="0>0|0|0|0" />
        <i3dMapping id="clearAreaHeight" node="0>0|0|0|1" />
        <i3dMapping id="levelArea1Start" node="0>0|1|0" />
        <i3dMapping id="levelArea1Width" node="0>0|1|0|0" />
        <i3dMapping id="levelArea1Height" node="0>0|1|0|1" />
        <i3dMapping id="indoorArea1Start" node="0>0|3|0" />
        <i3dMapping id="indoorArea1Width" node="0>0|3|0|0" />
        <i3dMapping id="indoorArea1Height" node="0>0|3|0|1" />
       
        <i3dMapping id="testArea1Start" node="0>0|4|0" />
        <i3dMapping id="testArea1End" node="0>0|4|0|0" />
        
        <i3dMapping id="tipOcclusionUpdateArea1Start" node="0>0|5|0" />
        <i3dMapping id="tipOcclusionUpdateArea1End" node="0>0|5|0|0" />
        <i3dMapping id="infoTrigger" node="0>0|6" />
        <i3dMapping id="hotspotMarker" node="0>0|10" />
        <i3dMapping id="teleportNode" node="0>0|11" />
        <i3dMapping id="loadTriggerAiNode" node="0>0|12|0|0" />
        <i3dMapping id="loadTriggerMarker" node="0>0|12|0|1" />
        <i3dMapping id="loadTrigger" node="0>0|12|0|2" />
        <i3dMapping id="unloadTriggerBunker" node="0>0|12|1|0" />
        <i3dMapping id="unloadTriggerBunkerMarker" node="0>0|12|1|1" />
        <i3dMapping id="baleTriggerBunker" node="0>0|12|1|2" />
        <i3dMapping id="bunkerFillVolume" node="0>0|12|1|3" />
        <i3dMapping id="unloadTriggerInFront" node="0>0|12|2|0" />
        <i3dMapping id="baleTriggerInFront" node="0>0|12|2|1" />
        <i3dMapping id="unloadTriggerInFrontAINode" node="0>0|12|2|2" />
        <i3dMapping id="unloadTriggerLiquidManure" node="0>0|12|3|0" />
        <i3dMapping id="unloadTriggerLiquidManureMarker" node="0>0|12|3|1" />
        <i3dMapping id="unloadTriggerLiquidManureAiNode" node="0>0|12|3|2" />
        <i3dMapping id="playerTrigger" node="0>0|12|4" />
        <i3dMapping id="playerTriggerMarker" node="0>0|12|4|0" />
		<i3dMapping id="chargeTrigger" node="0>2|0" />
        <i3dMapping id="chargeFlash" node="0>2|1" />
        <i3dMapping id="chargeLight" node="0>2|2" />
        <i3dMapping id="chargingAInode" node="0>2|3" />
        <i3dMapping id="fillTrigger" node="0>4|2" />		
		
    </i3dMappings>
</placeable>
