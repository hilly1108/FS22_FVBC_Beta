<licensePlates xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shared/xml/schema/mapLicensePlates.xsd">
    <font name="LICENSE_PLATE_US" />

    <licensePlate filename="maps/licensePlates/licensePlatesBC.i3d" node="0" type="SQUARISH" width="0.3" height="0.15">
        <font size="0.07" scaleX="0.8" scaleY="1"/>

        <variations>
            <variation> <!-- 3 numbers, 3 letters -->
                <value node="0|0" posX="-0.106" numerical="true"  alphabetical="false" />
                <value node="0|1" posX="-0.069" numerical="true"  alphabetical="false" />
                <value node="0|2" posX="-0.032" numerical="true"  alphabetical="false" />

                <value node="0|3" posX="0.032"  numerical="false" alphabetical="true" nextSection="true"/>
                <value node="0|4" posX="0.069"  numerical="false" alphabetical="true" />
                <value node="0|5" posX="0.106"  numerical="false" alphabetical="true" />

                <value node="0|6" character="_" locked="true"/>
                <value node="0|7" character="_" locked="true"/>
                <value node="0|8" character="_" locked="true"/>
            </variation>
            <variation> <!-- fantasy license plate -->
                <value node="0|0" posX="-0.123" numerical="true" alphabetical="true" special="true"/>
                <value node="0|1" posX="-0.092" numerical="true" alphabetical="true" special="true"/>
                <value node="0|2" posX="-0.061" numerical="true" alphabetical="true" special="true"/>
                <value node="0|3" posX="-0.030" numerical="true" alphabetical="true" special="true"/>
                <value node="0|4" posX="0.000"  numerical="true" alphabetical="true" special="true"/>
                <value node="0|5" posX="0.030"  numerical="true" alphabetical="true" special="true"/>
                <value node="0|6" posX="0.061"  numerical="true" alphabetical="true" special="true"/>
                <value node="0|7" posX="0.092"  numerical="true" alphabetical="true" special="true"/>
                <value node="0|8" posX="0.123"  numerical="true" alphabetical="true" special="true"/>
            </variation>
        </variations>

        <frame node="2" widthOffset="0.005" heightOffsetTop="0.005" heightOffsetBot="0.015"/>
    </licensePlate>

    <colorConfigurations shaderParameter="colorMat4" useDefaultColors="true">
        <colorConfiguration name="$l10n_ui_colorBlue" color="0.0075 0.0212 0.1845"/>
    </colorConfigurations>

    <placement defaultType="none"/>
</licensePlates>
